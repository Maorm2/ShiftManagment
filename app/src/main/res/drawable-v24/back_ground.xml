<?xml version="1.0" encoding="utf-8"?>


<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

<item>
    <shape android:shape="rectangle">
        <solid
            android:color="#7A5F71E6" />
        <corners
            android:radius="1dp"/>
    </shape>
</item>

<item android:right="1dp" android:left="1dp" android:bottom="2dp">
    <shape
        android:shape="rectangle">
        <gradient
            android:type="linear"
            android:startColor="@color/colorstyle1"
            android:endColor="@color/colorstyle3"
            android:angle="45"/>
        <corners
            android:radius="0dp"/>
    </shape>

</item>


</layer-list>
